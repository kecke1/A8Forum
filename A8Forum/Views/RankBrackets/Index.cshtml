@model IEnumerable<RankBracketViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MinRank)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaxRank)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Class)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MinRank)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaxRank)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Class)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RankBracketId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RankBracketId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RankBracketId">Delete</a>
            </td>
        </tr>
    }
    </tbody>
</table>